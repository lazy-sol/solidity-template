{
  "name": "solidity-template",
  "version": "1.0.2",
  "description": "Preconfigured Solidity dev env: Hardhat/Web3/Truffle, Infura, Etherscan",
  "main": "index.js",
  "engines": {
    "node": ">=16.20.0"
  },
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "deploy-goerli": "hardhat deploy --network goerli",
    "verify-goerli": "hardhat etherscan-verify --network goerli --api-key $ETHERSCAN_KEY",
    "deploy-mumbai": "hardhat deploy --network mumbai",
    "verify-mumbai": "hardhat etherscan-verify --network mumbai --api-key $POLYSCAN_KEY",
    "deploy-binance_testnet": "hardhat deploy --network binance_testnet",
    "verify-binance_testnet": "hardhat etherscan-verify --network binance_testnet --api-key $BSCSCAN_KEY",
    "deploy-mainnet": "hardhat deploy --network mainnet",
    "verify-mainnet": "hardhat etherscan-verify --network mainnet --api-key $ETHERSCAN_KEY",
    "deploy-polygon": "hardhat deploy --network polygon",
    "verify-polygon": "hardhat etherscan-verify --network polygon --api-key $POLYSCAN_KEY",
    "deploy-binance": "hardhat deploy --network binance",
    "verify-binance": "hardhat etherscan-verify --network binance --api-key $BSCSCAN_KEY"
  },
  "keywords": [],
  "author": "Basil Gorin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vgorin/solidity-template.git"
  },
  "bugs": {
    "url": "https://github.com/vgorin/solidity-template/issues"
  },
  "homepage": "https://github.com/vgorin/solidity-template#readme",
  "devDependencies": {
    "@lazy-sol/access-control": "github:lazy-sol/access-control",
    "@lazy-sol/access-control-upgradeable": "github:lazy-sol/access-control-upgradeable",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/test-helpers": "^0.5.15",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "hardhat": "^2.18.3",
    "hardhat-dependency-compiler": "^1.1.3",
    "hardhat-dependency-injector": "^1.0.0",
    "hardhat-deploy": "^0.11.43",
    "loglevel": "^1.8.1",
    "solidity-coverage": "^0.8.4"
  },
  "overrides": {
    "minimatch": "^3.0.5",
    "yargs-parser": "^5.0.1",
    "flat": "^5.0.1",
    "eth-gas-reporter": "^0.0.5",
    "lodash": "^4.17.20",
    "shelljs": "^0.8.4"
  }
}
